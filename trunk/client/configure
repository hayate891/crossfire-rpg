           echo $srcdir/$f
         else
           # /dev/null tree
           { { echo "$as_me:8824: error: cannot find input file: $f" >&5
echo "$as_me: error: cannot find input file: $f" >&2;}
   { (exit 1); exit 1; }; }
         fi;;
      esac
    done` || { (exit 1); exit 1; }
